{
  "private": "{{ npm_private }}",
  "files": [
    "!**/*.json",
    "!**/*.spec*",
    "!dist/bin",
    "!dist/main.bundled.js",
    "!dist/main.minified.js",
    "bin",
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/main.js",
  "bin": {
    "__pkg_{{ bin_name }}": "bin/cli",
    "{{ bin_name }}": "bin/cli"
  },
  "scripts": {},
  "typings": "dist/main.d.ts",
  "man": "dist/man",
  "dependencies": {
    "@nestjs/common": "8.1.2",
    "@nestjs/config": "^1.0.3",
    "@nestjs/core": "8.1.2",
    "@nestjs/event-emitter": "^1.0.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cosmiconfig": "^7.0.1",
    "nest-commander": "^2.3.3",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@megabytelabs/eslint-config": "^1.0.67",
    "@megabytelabs/jest-preset": "^1.0.0",
    "@megabytelabs/prettier-config": "^14.14.14",
    "@types/node": "^15.0.1",
    "@types/shelljs": "^0.8.9",
    "@types/update-notifier": "^5.1.0",
    "esbuild-node-externals": "^1.3.0",
    "nest-commander-schematics": "^2.0.1",
    "nest-commander-testing": "^2.0.1",
    "remark-man": "^8.0.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "@megabytelabs/jest-preset",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.common/assets/logo-icon-square-200x200.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": [
    "bin",
    "cli",
    "npm",
    "node",
    "nest",
    "commander",
    "inquirer",
    "manpage",
    "nestjs",
    "standalone",
    "cosmicconfig"
  ],
  "pkg": {
    "compress": "Brotli",
    "outputPath": "dist/bin"
  },
  "publishConfig": {
    "access": "{{ npm_publish_config_access }}"
  },
  "nodemonConfig": {
    "exec": "nest build && node --inspect-brk dist/main",
    "ext": "js,jsx,json,ts,tsx,yml",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "watch": [
      ".env",
      "src"
    ]
  }
}
