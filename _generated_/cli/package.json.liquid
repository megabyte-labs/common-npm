{
  "author": "{{author.name}} <{{author.email}}> ({{link.home}})",
  "bin": {
    "common-shared": "bin/cli"
  },
  "blueprint": {
    "description": "An NPM package that is part of the Megabyte Labs eco-system",
    "group": "Repository group here (e.g. ansible, npm, python)",
    "name": "My Project Name",
    "overview": "This text shows up at the top of the README.md.",
    "repository": {
      "github": "{{#if repository.github}}{{repository.github}}{{else}}https://github.com/{{profile.github}}/{{repository.prefix.github}}{{slug}}{{/if}}",
      "gitlab": "{{#if repository.gitlab}}{{repository.gitlab}}{{else}}{{repository.gitlabBaseUrl}}/{{group}}/{{subgroup}}/{{slug}}{{/if}}"
    },
    "slug": "my-project",
    "subgroup": "Repository subgroup here (e.g. cli, library, plugin)",
    "title": "Add title of the README.md here"
  },
  "bugs": {
    "email": "{{email.help}}",
    "url": "{{#if repository.gitlab}}{{repository.gitlab}}{{else}}{{repository.gitlabBaseUrl}}/{{group}}/{{subgroup}}/{{slug}}{{/if}}{{repository.location.issues.gitlab}}"
  },
  "changelog": {
    "displayTypes": [
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert"
    ],
    "showAuthor": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "helpUrl": "https://megabyte.space/docs/contributing/commits"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji-conventional"
    }
  },
  "dependencies": {
    "@nestjs/common": "8.1.2",
    "@nestjs/config": "^1.0.3",
    "@nestjs/core": "8.1.2",
    "chalk": "^4.1.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cosmiconfig": "^7.0.1",
    "nest-commander": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "shelljs": "^0.8.4",
    "update-notifier": "^5.1.0"
  },
  "description": "{{description}}",
  "devDependencies": {
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/node": "^16.11.6",
    "@types/shelljs": "^0.8.9",
    "@types/update-notifier": "^5.1.0",
    "@washingtondc/development": "latest",
    "cz-emoji-conventional": "^1.0.1",
    "esbuild": "^0.12.29",
    "esbuild-node-externals": "^1.3.0",
    "eslint-config-strict-mode": "latest",
    "glob": "^7.2.0",
    "handlebars-helpers": "^0.10.0",
    "jest-preset-ts": "^1.0.0",
    "nest-commander-schematics": "^2.0.1",
    "nest-commander-testing": "^2.0.1",
    "prettier-config-sexy-mode": "latest",
    "remark-man": "^8.0.0",
    "semantic-release-config": "latest",
    "source-map-support": "^0.5.20"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "eslintConfig": {
    "extends": "eslint-config-strict-mode"
  },
  "files": [
    "bin/",
    "dist/",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "funding": [
    {
      "type": "opencollective",
      "url": "{{profile_link.opencollective}}/{{profile.opencollective}}"
    },
    {
      "type": "patreon",
      "url": "{{profile_link.patreon}}/{{profile.patreon}}"
    }
  ],
  "homepage": "{{link.home}}",
  "jest": {
    "preset": "jest-preset-ts",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "filename": "index.html",
          "logoImgPath": "./.config/assets/logo-jest-report.png",
          "openReport": true,
          "pageTitle": "Code Coverage Report",
          "publicPath": "./coverage"
        }
      ]
    ]
  },
  "keywords": [
    "bin",
    "cli",
    "commander",
    "cosmicconfig",
    "inquirer",
    "manpage",
    "mblabs",
    "megabytelabs",
    "nestjs",
    "professormanhattan",
    "washingtondc"
  ],
  "license": "{{license}}",
  "lint-staged": {
    "**/!(logo).(jpg|png)": [
      "task image:compress --"
    ],
    "**/*": [
      "task lint:any --"
    ],
    "**/*.(css|html|less|md|mjml|php|scss|sh|xml)": [
      "task fix:prettier --"
    ],
    "**/*.(js|jsx|ts|tsx)": [
      "task fix:js --",
      "task lint:js --"
    ],
    "**/*.json": [
      "task fix:json --"
    ],
    "**/*.sh": [
      "task lint:shell --"
    ],
    "**/*.toml": [
      "task lint:toml --"
    ],
    "**/*.xml": [
      "task fix:xml --",
      "task lint:xml --"
    ],
    "**/*.yml": [
      "task fix:yaml --",
      "task lint:yaml --"
    ],
    "logo.png": [
      "task image:resize -- 200 200",
      "task image:compress --"
    ]
  },
  "main": "dist/main.js",
  "man": "man",
  "name": "{{#if customPackageName}}{{customPackageName}}{{else}}@{{profile.npmjs_organization}}/{{repository.prefix.github}}{{slug}}{{/if}}",
  "nodemonConfig": {
    "exec": "tsc -p . && node --inspect-brk dist/main",
    "ext": "js,jsx,json,ts,tsx,yml",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "watch": [
      ".env",
      "src"
    ]
  },
  "ntl": {
    "descriptions": {
      "build": "Builds the project using the build command specified under scripts in package.json",
      "help": "Display the Bodega Taskfile.yml interactive help menu",
      "prepare": "A hook triggered by running npm install that runs logic such as installing pre-commit hooks",
      "repair": "Fixes basic issues in projects that are having trouble running basic commands like 'task start'",
      "start": "Entrypoint for new developers that installs requirements and then runs 'task start'",
      "test": "Runs the appropriate test logic by running the test command defined under scripts in package.json"
    }
  },
  "optionalDependencies": {},
  "pkg": {
    "compress": "Brotli",
    "outputPath": "build"
  },
  "prettier": "prettier-config-sexy-mode",
  "private": false,
  "publishConfig": {
    "@megabyte:registry": "https://gitlab.com/api/v4/projects/{{gitlab_project_id}}/packages/npm/",
    "access": "{{npm_publish_config_access}}"
  },
  "release": {
    "branches": [
      "main",
      "master",
      "next",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "extends": "semantic-release-config"
  },
  "repository": {
    "type": "git",
    "url": "git+{{#if repository.github}}{{repository.github}}{{else}}https://github.com/{{profile.github}}/{{repository.prefix.github}}{{slug}}{{/if}}.git"
  },
  "scripts": {
    "build": "{{{scriptsBuild}}}",
    "help": "{{{scriptsHelp}}}",
    "prepare": "{{{scriptsPrepare}}}",
    "repair": "bash <(curl -sS https://install.doctor/repair)",
    "start": "{{{scriptsStart}}}",
    "test": "{{{scriptsTest}}}"
  },
  "type": "{{npm_type}}",
  "typings": "dist/main.d.ts",
  "version": "{{version}}"
}
