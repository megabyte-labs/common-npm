import { DiscoveryService } from '@golevelup/nestjs-discovery';
import { Inquirer as InquirerType } from 'inquirer';
export declare class InquirerService {
    private readonly inquirer;
    private readonly discoveryService;
    constructor(inquirer: InquirerType, discoveryService: DiscoveryService);
    private readonly inquirerKeyToMetadataKeyMap;
    ask: <T>(questionSetName: string, options: Partial<T> | undefined) => Promise<T>;
    prompt<T>(questionSetName: string, options: Partial<T> | undefined): Promise<T>;
    private findQuestionSet;
    private mapMetaQuestionToQuestion;
    private parseRawQuestionMetadata;
}
